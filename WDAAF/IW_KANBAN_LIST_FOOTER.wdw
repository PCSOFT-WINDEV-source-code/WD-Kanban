         -
           code : |1+
            
            Card is kbCard
            
            // If the input is validated
            if WIN_KBCARD.Open(Card) = True then
            	
            	// Executes the add card process
            	if KanbanControl.ExecuteProcess( trtAddKanbanCard, Card, ListCard ) = False then
            		return
            	end
            	
            	// Adds the card in the list
            	nIndex is int = ListCard.AddCard( Card )
            	if nIndex > 0 then
            		
            		// Set the position on the new card		
            		ListCard.PositionList( ListCard.Card[ nIndex ] )
            	end
            end
