     p_codes :
      -
        internal_properties : CAAAAAgAAABW6mkYhVKQeKcQ+sgdMfRrQElIPbLqIwh0lHYIREj+77yW/3MTJxmyGoXNA0isOjlfXNDFQp8ubo4xi7/xAlBtXD68T88hqXchlw4QDSK3qBr43ueWsIbjozMCe31S21BUt4Pnwgw4/RQf4Pw3thBN0pHiy0kX9d/gyPeTcfM/Si3sxW3qtkpjho7cOYBOSAMS
        code : |1-
         
         // If the Kanban is empty
         if LIST_PENDING.Card.Empty _and_ LIST_INPROGRESS.Card.Empty _and_ LIST_DONE.Card.Empty then
         	
         	// If on a new Kanban
         	if KanbanFile = "" then
         		
         		// No backup	
         		return True
         	end	
         end
         
         // If there is no file
         if KanbanFile = "" then
         	
         	sFile is string
         	
         	// Opens the file picker
         	sFile = fSelect("", "", "Save as...", "All files (*.*)" + TAB + "*.*" + CR + "Kanban (*.wdkanban)" + TAB + "*.WDKANBAN", "WDKANBAN", fselCreate + fselExist)
         	if sFile = "" then		
         		return False
         	end		
         	
         	// Stores the name of the file	
         	KanbanFile = sFile
         end
         
         stKanbanDescription is KanbanDescription
         
         stKanbanDescription.ListPending 	<= LIST_PENDING.Card
         stKanbanDescription.ListInProgress 	<= LIST_INPROGRESS.Card
         stKanbanDescription.ListDone	<= LIST_DONE.Card
         
         // Serializes and saves to disk
         bufKanban is buffer
         Serialize(stKanbanDescription, bufKanban, psdXML)
         fSaveBuffer(KanbanFile, bufKanban)
         
         return True
        type : 18
