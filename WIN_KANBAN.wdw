        internal_properties : CAAAAAgAAADLKU3S5l2kiEzL18by6lUzbiiRy0U/yVZhc+/PNcN4wlOlvCyPvyfHsskY9gbo7nOTcY0ghU9pKmABlSXh+uAz18uFuoHsZKUZuRv8wAyh+vBBB8iGNyAspsc14xMS0G1DKU3wGhFK9rK3L++tUaCLjFZPP0fT1CXmpzWkHBK4np2jOC5CJYkZYnrxa7iS5H57rXSR0/WHiqiamghWfQ==
        code : |1+
         
         sFile is string
         
         // Opens the file picker
         sFile = fSelect("", "", "Save as...", "All files (*.*)" + TAB + "*.*" + CR + "Kanban (*.wdkanban)" + TAB + "*.WDKANBAN", "WDKANBAN", fselOpen + fselExist)
         if sFile = "" then
         	SetFocusAndReturnToUserInput()
         end		
         
         // Start on a blank Kanban
         BTN_NEW_KANBAN.ExecuteProcess(trtClick)
         
         // Stores
         KanbanFile = sFile
         
         stKanbanDescription	is KanbanDescription
         
         // Loads and deserializes
         bufKanban			is buffer	= fLoadBuffer(KanbanFile)
         Deserialize(stKanbanDescription, bufKanban, psdXML)
         
         // Add the cards in the Kanban
         stKanbanDescription.ListPending.Applique	( Card => LIST_PENDING.AddCard(Card) )
         stKanbanDescription.ListInProgress.Applique	( Card => LIST_INPROGRESS.AddCard(Card) )
         stKanbanDescription.ListDone.Applique	( Card => LIST_DONE.AddCard(Card) )
        type : 18
